{
	"name": "@accuser/unist-util-flatmap",
	"version": "1.0.0",
	"description": "Flatmap for unist nodes (useful for transforming trees) without mutating them.",
	"type": "module",
	"main": "dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"import": {
				"default": "./dist/index.js",
				"types": "./dist/index.d.ts"
			},
			"require": {
				"default": "./dist/cjs/index.js",
				"types": "./dist/cjs/index.d.ts"
			}
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "duel",
		"format": "prettier --write {src,test}/**/*.{js,ts}",
		"lint": "eslint",
		"test": "vitest"
	},
	"keywords": [
		"unist",
		"flatmap"
	],
	"author": {
		"email": "mhgibbons@me.com",
		"name": "Matthew Gibbons",
		"url": "https://github.com/accuser"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/accuser/unist-util-flatmap.git"
	},
	"bugs": {
		"url": "https://github.com/accuser/unist-util-flatmap/issues"
	},
	"devDependencies": {
		"@knighted/duel": "^1.0.8",
		"@types/eslint": "^9.6.1",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/unist": "^3.0.3",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.1.0",
		"prettier": "^3.3.3",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.4.0",
		"unist-builder": "^4.0.0",
		"vitest": "^2.0.5"
	}
}
